{
  "applicationId": "js.neutralino.zero",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/www/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": [
    "app.*",
    "os.getEnv",
    "filesystem.readFile",
    "filesystem.getJoinedPath"
  ],
  "modes": {
    "window": {
      "title": "fm-japanese-input",
      "width": 730,
      "height": 790,
      "minWidth": 600,
      "minHeight": 400,
      "icon": "/www/icon.png",
      "enableInspector": false
    }
  },
  "cli": {
    "binaryName": "fm-japanese-input",
    "resourcesPath": "/www/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/www/neutralino.js",
    "binaryVersion": "6.3.0",
    "clientVersion": "6.3.0",
    "processArgs": "--env-file=.env"
  }
}
